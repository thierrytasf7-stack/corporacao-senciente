# AIOS-Core Environment Configuration
# Copie para .env e configure suas chaves

# OpenRouter API (para execução de agentes LLM)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Modelo padrão
AIOS_DEFAULT_MODEL=claude-3.5-sonnet

# Temperatura padrão
AIOS_DEFAULT_TEMPERATURE=0.3

# Max tokens
AIOS_MAX_TOKENS=8000

# Logging
AIOS_LOG_LEVEL=info
