# ============================================
# CORPORACAO SENCIENTE - ARETE CONFIGURATION
# Template file - DO NOT COMMIT SECRETS
# ============================================

# Qdrant Vector DB
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Feature Flags Industry 6.0/7.0
VECTOR_BACKEND=qdrant
MEM0_ENABLED=true
SENSORY_FEEDBACK_ENABLED=true
CORPORATE_WILL_ENABLED=true

# Trading Integration
TRADING_ENABLED=true
TRADING_MAX_LEVERAGE=20
TRADING_TESTNET_ONLY=true

# Binance Configuration (from BINANCE-BOT)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
BINANCE_USE_TESTNET=true
BINANCE_TESTNET_URL=https://testnet.binance.vision
BINANCE_BASE_URL=https://api.binance.com

# Risk Management
RISK_MAX_POSITION_SIZE_PERCENT=5
RISK_STOP_LOSS_PERCENT=2
RISK_TAKE_PROFIT_PERCENT=6
RISK_MAX_DAILY_LOSS_PERCENT=10
RISK_MAX_DRAWDOWN_PERCENT=20

# Corporate Will Settings
CORPORATE_WILL_TRADE_THRESHOLD_USD=100
CORPORATE_WILL_REQUIRE_APPROVAL=true
CORPORATE_WILL_AUTO_REJECT_ETHICAL=true

# L.L.B. Protocol
LLB_PROTOCOL_ENABLED=true
LLB_MEMORY_SYNC_INTERVAL=60

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# LLM APIs
GROK_API_KEY=your_grok_api_key
GEMINI_API_KEY=your_gemini_api_key
SERPER_API_KEY=your_serper_api_key
TAVILY_API_KEY=your_tavily_api_key

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true
