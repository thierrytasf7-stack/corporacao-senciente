# Chaves Supabase (não commitar chaves reais)
SUPABASE_URL=https://<project>.supabase.co
SUPABASE_ANON_KEY=<publishable_or_anon_key>
SUPABASE_SERVICE_ROLE_KEY=<service_role_key>

# Configuração MCP / LangGraph
MCP_SUPABASE_PROJECT_REF=<project_ref>
MCP_SUPABASE_SCHEMA=public
MCP_EMBEDDING_MODEL=Xenova/bge-small-en-v1.5
GROK_API_KEY=<gsk_xxx>
GROK_MODEL=grok-beta
GROK_TIMEOUT_MS=20000
GROK_MAX_RETRIES=2
GROK_FALLBACK_MODEL=grok-beta
LOG_BOARDROOM=true
LOG_ALIGN_ALERTS=false
GEMINI_API_KEY=<your_gemini_key>
GEMINI_MODEL=gemini-1.5-flash
METRIC_LOG_LLM=false

# Ajustes de vetores
EMBEDDING_DIM=384
IVFFLAT_LISTS=100
IVFFLAT_PROBES=10

# Copywriting Agent - Grammar & Language
LANGUAGETOOL_API_URL=https://api.languagetool.org/v2/check
LANGUAGETOOL_USE_LOCAL=false
# LANGUAGETOOL_LOCAL_URL=http://localhost:8081/v2/check

# Copywriting Agent - Sentiment Analysis
# Criar conta: https://huggingface.co
# Gerar token: https://huggingface.co/settings/tokens
# HUGGINGFACE_API_KEY=your_key_here
HUGGINGFACE_SENTIMENT_MODEL=cardiffnlp/twitter-roberta-base-sentiment-latest

# Copywriting Agent - Content Publishing
# WORDPRESS_URL=https://your-site.com
# WORDPRESS_USERNAME=your_username
# WORDPRESS_APP_PASSWORD=your_app_password

# Copywriting Agent - Analytics
# GOOGLE_ANALYTICS_PROPERTY_ID=your_property_id
# GOOGLE_ANALYTICS_CLIENT_ID=your_client_id
# GOOGLE_ANALYTICS_CLIENT_SECRET=your_client_secret
# GOOGLE_ANALYTICS_REFRESH_TOKEN=your_refresh_token

# Copywriting Agent - Templates & Knowledge
# NOTION_API_KEY=your_notion_key
# NOTION_DATABASE_ID=your_database_id

# ⚠️ DESCONTINUADO: Jira, Confluence, GitKraken
# Substituído por Protocolo L.L.B. (Letta, LangMem, ByteRover)
# Jira → Letta (usa SUPABASE_URL e SUPABASE_SERVICE_ROLE_KEY)
# Confluence → LangMem (usa SUPABASE_URL e SUPABASE_SERVICE_ROLE_KEY)
# GitKraken → ByteRover + Git Nativo (sem variáveis adicionais)
# ATLASSIAN_SITE=https://your-site.atlassian.net
# ATLASSIAN_CLOUD_ID=your_cloud_id
# ATLASSIAN_API_TOKEN=your_api_token
# ATLASSIAN_ORG_ID=your_org_id
# ATLASSIAN_EMAIL=your_email@example.com

# ByteRover Cipher (Self-Hosted) - Opcional
# BYTEROVER_CIPHER_URL=http://localhost:8080
# BYTEROVER_CIPHER_API_KEY=your_api_key

