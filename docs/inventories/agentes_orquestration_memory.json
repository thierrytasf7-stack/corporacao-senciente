          "name": "corporate_risks",
          "columns": [
            {
              "name": "id",
              "type": "uuid",
              "primary": true,
              "default": "gen_random_uuid()"
            },
            {
              "name": "title",
              "type": "varchar(255)",
              "nullable": false
            },
            {
              "name": "description",
              "type": "text"
            },
            {
              "name": "category",
              "type": "varchar(100)"
            },
            {
              "name": "severity",
              "type": "varchar(20)"
            },
            {
              "name": "probability",
              "type": "decimal",
              "check": "probability >= 0 AND probability <= 1"
            },
            {
              "name": "impact",
              "type": "decimal",
              "check": "impact >= 0 AND impact <= 1"
            },
            {
              "name": "risk_score",
              "type": "decimal",
              "generated": "ALWAYS AS (probability * impact) STORED"
            },
            {
              "name": "mitigation_plan",
              "type": "text"
            },
            {
              "name": "owner_id",
              "type": "uuid",
              "references": "auth.users(id)"
            },
            {
              "name": "status",
              "type": "varchar(50)",
              "default": "'identified'"
            },
            {
              "name": "created_at",
              "type": "timestamp with time zone",
              "default": "NOW()"
            },
            {
              "name": "updated_at",
              "type": "timestamp with time zone",
              "default": "NOW()"
            }
          ],
          "indexes": [
            "category",
            "severity",
            "status",
            "risk_score"
          ],
          "constraints": [
            "severity IN ('CRITICAL', 'HIGH', 'MEDIUM', 'LOW')",
            "status IN ('identified', 'mitigated', 'accepted', 'transferred')"
          ]
        }
      }
