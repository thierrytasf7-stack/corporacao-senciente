# Environment
NODE_ENV=development

# Server - Diana port range 21300-21399
PORT=21341
DIANA_BINANCE_BACKEND_PORT=21341
HOST=localhost

# Database
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/aura_trading
DB_HOST=localhost
DB_PORT=5432
DB_NAME=aura_trading
DB_USER=postgres
DB_PASSWORD=your_postgres_password_here

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=your_jwt_secret_here_min_32_chars
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here_min_32_chars
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Encryption (must be 32+ chars)
ENCRYPTION_KEY=your_encryption_key_here_min_32_chars!

# Binance API - TESTNET
# Get keys from https://testnet.binance.vision/
BINANCE_API_KEY=your_binance_testnet_api_key
BINANCE_SECRET_KEY=your_binance_testnet_secret_key
BINANCE_BASE_URL=https://api.binance.com
BINANCE_TESTNET_URL=https://testnet.binance.vision
BINANCE_FUTURES_TESTNET_URL=https://testnet.binancefuture.com
BINANCE_USE_TESTNET=true
BINANCE_TESTNET=true

# API URL for trigger-monitor
API_URL=http://localhost:21341/api/v1

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# CORS - Diana frontend port
CORS_ORIGIN=http://localhost:21340

# WebSocket (same as backend port)
WS_PORT=21341