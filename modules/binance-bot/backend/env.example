# Configurações do Banco de Dados
DATABASE_URL=postgresql://aura_user:aura_password@localhost:5432/aura_db_dev

# Configurações do Redis
REDIS_URL=redis://localhost:6379

# Configurações JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production

# Configurações de Criptografia
ENCRYPTION_KEY=your-32-character-encryption-key-here

# Configurações da Binance Testnet
BINANCE_API_KEY=your_binance_testnet_api_key_here
BINANCE_SECRET_KEY=your_binance_testnet_secret_key_here
BINANCE_USE_TESTNET=true
BINANCE_API_URL=https://testnet.binance.vision
BINANCE_WS_URL=wss://testnet.binance.vision/ws

# Configurações do Servidor
PORT=3001
NODE_ENV=development
LOG_LEVEL=debug

# Configurações de Segurança
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
SESSION_TIMEOUT=3600000
