{
  "name": "historical-data-pipeline",
  "version": "1.0.0",
  "description": "ETL pipeline for loading historical betting data from Betfair API",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate": "psql -h localhost -U postgres -d betting_platform -f src/database/migrations/001_create_historical_tables.sql",
    "constraints": "psql -h localhost -U postgres -d betting_platform -f src/database/migrations/002_add_constraints.sql",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^8.2.1",
    "luxon": "^3.3.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "etl",
    "betting",
    "betfair",
    "historical-data",
    "postgresql"
  ],
  "author": "Diana Corporacao Senciente",
  "license": "MIT"
}
