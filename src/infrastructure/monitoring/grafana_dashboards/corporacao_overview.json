{
  "dashboard": {
    "id": null,
    "title": "üè≠ Corpora√ß√£o Senciente - Overview",
    "tags": ["corporacao", "overview", "business"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Holding Performance Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "holding_total_revenue",
            "legendFormat": "Total Revenue"
          },
          {
            "expr": "holding_total_profit",
            "legendFormat": "Total Profit"
          },
          {
            "expr": "holding_cash_position",
            "legendFormat": "Cash Position"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "red", "value": 80 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Pipeline Friction Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "pipeline_friction_score",
            "legendFormat": "Friction Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 40 },
                { "color": "red", "value": 70 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 3,
        "title": "System Health Overview",
        "type": "table",
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{ job }}",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": { "text": "DOWN", "color": "red" },
                  "1": { "text": "UP", "color": "green" }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 0, "y": 8 }
      },
      {
        "id": 4,
        "title": "Resource Usage",
        "type": "bargauge",
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "(1 - avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m]))) * 100",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(1 - node_filesystem_avail_bytes / node_filesystem_size_bytes) * 100",
            "legendFormat": "Disk Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 70 },
                { "color": "red", "value": 90 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 8, "y": 8 }
      },
      {
        "id": 5,
        "title": "Request Rate & Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "Request Rate"
          },
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "Error Rate"
          }
        ],
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 }
      },
      {
        "id": 6,
        "title": "Subsidiary Performance",
        "type": "table",
        "targets": [
          {
            "expr": "subsidiary_health_score",
            "legendFormat": "{{ subsidiary_id }}",
            "instant": true
          },
          {
            "expr": "subsidiary_monthly_revenue",
            "legendFormat": "{{ subsidiary_id }} Revenue",
            "instant": true
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 7,
        "title": "AI Model Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "ai_model_inference_time",
            "legendFormat": "Inference Time ({{ model }})"
          },
          {
            "expr": "ai_model_success_rate",
            "legendFormat": "Success Rate ({{ model }})"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 8,
        "title": "Business Metrics Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "holding_total_revenue",
            "legendFormat": "Revenue"
          },
          {
            "expr": "holding_total_profit",
            "legendFormat": "Profit"
          },
          {
            "expr": "subsidiary_creation_success_rate",
            "legendFormat": "Creation Success Rate"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
      },
      {
        "id": 9,
        "title": "Alert Summary",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS",
            "legendFormat": "{{ alertname }}",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "red", "value": 1 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 12, "x": 12, "y": 24 }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}